{
  "name": "tenso",
  "description": "Tenso is an HTTP REST API framework",
  "version": "17.0.0",
  "homepage": "https://github.com/avoidwork/tenso",
  "author": "Jason Mulligan <jason.mulligan@avoidwork.com>",
  "repository": {
    "type": "git",
    "url": "git://github.com/avoidwork/tenso.git"
  },
  "bugs": {
    "url": "https://github.com/avoidwork/tenso/issues"
  },
  "license": "BSD-3-Clause",
  "engineStrict": true,
  "engines": {
    "node": ">=16.0.0"
  },
  "main": "dist/tenso.cjs",
  "module": "dist/tenso.esm.js",
  "type": "module",
  "sourceType": "module",
  "types": "types/tenso.d.ts",
  "files": [
    "dist",
    "types",
    "www"
  ],
  "scripts": {
    "benchmark": "node benchmark.js",
    "build": "mkdir -p dist && rm -rf dist/* && npm run rollup",
    "changelog": "auto-changelog -p",
    "coverage": "nyc npm run test",
    "lint": "eslint *.js src/*.js test/*.js",
    "mocha": "mocha test/*.js",
    "rollup": "rollup --config",
    "sample": "node sample.js",
    "test": "npm run lint && npm run mocha",
    "types": "npx -p typescript tsc src/*.js --declaration --allowJs --emitDeclarationOnly --outDir types"
  },
  "dependencies": {
    "@fast-csv/format": "^4.3.5",
    "connect-redis": "^6.1.3",
    "cookie-parser": "^1.4.6",
    "express-session": "^1.17.3",
    "keysort": "^1.0.2",
    "lusca": "^1.7.0",
    "mime-db": "^1.52.0",
    "passport": "^0.6.0",
    "passport-http": "^0.3.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "passport-oauth2": "^1.6.1",
    "passport-saml": "^3.2.1",
    "precise": "^2.0.0",
    "redis": "^4.3.1",
    "tiny-coerce": "^2.0.0",
    "tiny-etag": "^3.0.1",
    "tiny-eventsource": "^1.2.1",
    "tiny-merge": "^1.0.0",
    "woodland": "^17.0.1",
    "xml": "^1.0.1",
    "yamljs": "^0.3.0",
    "yargs-parser": "^21.1.1"
  },
  "devDependencies": {
    "auto-changelog": "^2.4.0",
    "autocannon": "^7.10.0",
    "concurrently": "^7.4.0",
    "eslint": "^8.24.0",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "rollup": "^2.79.1",
    "rollup-plugin-terser": "^7.0.2",
    "tiny-httptest": "^3.0.0"
  },
  "keywords": [
    "rest",
    "api",
    "cqrs",
    "gateway",
    "server",
    "hypermedia",
    "framework"
  ]
}
